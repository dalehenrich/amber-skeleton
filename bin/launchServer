#!/bin/sh

# image name (arg #1) is relative to `../server/pharo` directory
# path (arg $2) is relative to `../server/pharo` directory

ROOT=`dirname $0`
cd $ROOT/../server/pharo

OS=`uname`
echo $OS
case  $OS in
  Darwin*)
    VM="/Users/dhenrich/Pharo/CogVM.app/Contents/MacOS/CogVM";;
  Linux*)
    VM="/export/foos1/users/dhenrich/pharo/cog/coglinux/bin/squeak";;
esac

echo $VM

$VM $1 $2

